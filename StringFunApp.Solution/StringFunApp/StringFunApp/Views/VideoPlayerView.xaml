             xmlns:video="clr-namespace:FormsVideoLibrary"
             xmlns:bhv="clr-namespace:StringFunApp.Behaviors"
             xmlns:local="clr-namespace:StringFunApp.ClassLibrary.ViewModels"
             x:Class="StringFunApp.Views.VideoPlayerView">
    <ContentPage.Content>
        <FlexLayout>
            <video:VideoPlayer x:Name="videoPlayer" AutoPlay="true"></video:VideoPlayer>
        </FlexLayout>
    </ContentPage.Content>
</ContentPage>
